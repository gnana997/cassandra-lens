{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": false,
    "outDir": "./dist"
  },
  "include": [
    "src/webviews/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
